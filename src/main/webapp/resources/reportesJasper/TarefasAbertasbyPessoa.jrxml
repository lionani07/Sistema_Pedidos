		<queryString language="SQL">
			<![CDATA[select pessoa.people, pessoa.rol, pessoa.nome, COUNT(pessoa.people) as TOTAL,
   Sum(case when Tarefa.estado = 'CREADA'  then 1 else 0 End) as CREADA, 
   Sum(case when Tarefa.estado = 'APROBADA' then 1 else 0 End) as APROBADA,
   Sum(case when tarefa.estado = 'INICIADA' then 1 else 0 end) as INICIADA  
  from tarefa, pessoa WHERE tarefa.id_subLider = pessoa.id AND tarefa.estado <> 'FECHADA' GROUP BY pessoa.people, pessoa.rol, pessoa.nome]]>
		</queryString>
